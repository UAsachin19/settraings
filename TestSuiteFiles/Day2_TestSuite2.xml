<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Day2Suite" parallel="none">

<listeners>
      <listener class-name="org.uncommons.reportng.HTMLReporter"/>
      <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
</listeners>

  <test name="HsbcDynamicMenuBorrowing" enabled="false">
  	
  	<parameter name="BrowserToBeOpened" value="chrome"></parameter>
  	<parameter name="AppUrl" value="https://www.hsbc.co.in"></parameter>
  	  	  	
    <classes>
      <class name="com.hsbc.testcases.Day2_HsbcDynamicMenuBorrowing"/>
    </classes>
  </test> <!-- TitleValidation -->
  
  <test name="BingValidation" enabled="true">
  	
  	<parameter name="BrowserToBeOpened" value="chrome"></parameter>
  	<parameter name="AppUrl" value="https://www.bing.com/"></parameter>
  	  	  	
    <classes>
      <class name="com.hsbc.testcases.Day2_BingValidation"/>
    </classes>
  </test> <!-- TitleValidation -->
  
  
    
  
</suite> <!-- Day1Suite -->
